<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Form\ValueHolder"/>
        <service id="helpmee.system.form.agent.type" class="Form\ValueSender"/>
    </services>
</container>
-----
services:
    form.valueholder:
        class: Form\ValueHolder

    helpmee.system.form.agent.type:
        class: Form\ValueSender
